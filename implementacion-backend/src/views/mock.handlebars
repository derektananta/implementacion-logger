<div>
  <h2 class="page_title">Mock Products</h2>
  <div class="productsContainer">
    {{#each products}}
    <div class="product">
      <h3>{{this.title}}</h3>
      <p>Price: ${{this.price}}</p>
      <p>Category: {{this.category}}</p>
    </div>
    {{/each}}
  </div>
  {{#if prevLink}}
  <a href="{{prevLink}}">Previous Page</a>
  {{/if}}
  {{#if nextLink}}
  <a href="{{nextLink}}">Next Page</a>
  {{/if}}
</div>
